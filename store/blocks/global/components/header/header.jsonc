{
  "header": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header.full": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header-layout.desktop": {
    "title": "Header Desktop",
    "children": [
      "header-row#header__top",
      "header-row#header__middle",
      "header-row#header__bottom"
    ]
  },
  "header-layout.mobile": {
    "title": "Header Mobile",
    "children": [
      "header-row#header__top",
      // "header-row#header__middle",
      "header-row#header__middle--mobile",
      "header-row#header__middle--mobile--search-bar"
      // "header-row#header__bottom"
    ]
  },
  "header-row#header__top": {
    "title": "Header Superior",
    "children": [
      "link#header__top--discount"
    ],
    "props": {
      "blockClass": "header__top--discount",
      "fullWidth": true,
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true
    }
  },
  "logo#header__top--logo": {
    "title": "logo",
    "props": {
      "url": "assets/img/logo.png",
      "href": "/",
      "width": 170,
      "blockClass": "header__top--logo"
    }
  },
  "search-bar": {
    "props": {
      "placeholder": "Â¿QuÃ© estÃ¡s buscando?",
      "blockClass": "search-bar-mod"
    }
  },
  "login": {
    "props": {
      "emailAndPasswordTitle": "LOG-IN",
      "accessCodeTitle": "Acess Code LOG-IN",
      "emailPlaceholder": "e-mail",
      "passwordPlaceholder": "password",
      "showPasswordVerificationIntoTooltip": true,
      "showIconProfile": true,
      "iconLabel": "Mi cuenta"
    }
  },
  "header-row#header__middle": {
    "title": "Header Medio",
    "children": [
      // "drawer#header__mobile-menu",
      "logo#header__top--logo",
      "header-spacer",
      "search-bar",
      "header-spacer",
      "login",
      "minicart.v2"
    ],
    "props": {
      "blockClass": "header__middle",
      "fullWidth": true
    }
  },
  "header-row#header__middle--mobile": {
    "title": "Header Medio Mobile",
    "children": [
      "drawer#header__mobile-menu",
      "logo#header__top--logo",
      "header-spacer",
      "login",
      "minicart.v2"
    ],
    "props": {
      "blockClass": "header__middle--mobile",
      "fullWidth": true
    }
  },
  "header-row#header__middle--mobile--search-bar": {
    "title": "Header Medio Mobile Search Bar",
    "children": [
      "search-bar"
    ],
    "props": {
      "blockClass": "header__middle--mobile--search-bar",
      "fullWidth": true
    }
  },
  "link#header__top--discount": {
    "title": "Link de descuento",
    "props": {
      "blockClass": "link__discount--promo",
      "href": "/sale",
      "label": "SALE Hasta 50% OFF en ref seleccionadas ðŸšš",
      "openNewTab": false
    }
  },
  "drawer#header__mobile-menu": {
    "title": "Mobile Menu",
    "children": [
      "menu#header__mobile-menu--menu"
    ],
    "props": {
      "title": "Mobile-Menu",
      "blockClass": "header__mobile-menu",
      "fullWidth": true
    }
  },
  "menu#header__mobile-menu--menu": {
    "title": "Menu",
    "props": {
      "orientation": "vertical",
      "blockClass": "header__mobile-menu--menu",
      "fullWidth": true
    },
    "children": [
      "menu-item#category__one",
      "menu-item#category__two"
    ]
  },
  "menu-item#category__one": {
    "title": "Menu Item Category One",
    "props": {
      "id": "category__one",
      "type": "custom",
      "itemProps": {
        "type": "internal",
        "href": "/category/one",
        "tagTitle": "Category One Tag",
        "text": "Category One Text",
        "label": "Category One",
        "blockClass": "category__one"
      }
    }
  },
  "menu-item#category__two": {
    "title": "Menu Item Category Two",
    "props": {
      "id": "category__two",
      "type": "custom",
      "itemProps": {
        "type": "internal",
        "href": "/category/two",
        "tagTitle": "Category Two Tag",
        "text": "Category Two Text",
        "label": "Category Two",
        "blockClass": "category__two"
      }
    }
  },
  "header-row#header__bottom": {
    // Parte inferior del header en desktop
    "title": "Header Inferior",
    "children": [
      "vtex.menu@2.x:menu#header__bottom--menu"
    ],
    "props": {
      "blockClass": "header__bottom",
      "fullWidth": true
    }
  },
  "vtex.menu@2.x:menu#header__bottom--menu": {
    // Menu de la parte inferior del header en desktop
    "title": "Menu",
    "props": {
      "orientation": "horizontal",
      "blockClass": "header__bottom--menu",
      "fullWidth": true
    },
    "children": [
      "menu-item#department__man",
      "menu-item#department__woman",
      "menu-item#department__news",
      "header-spacer",
      "menu-item#department__sustainable",
      "menu-item#department__topproducts",
      "menu-item#department__sale"
    ]
  },

  "menu-item#department__man": {
