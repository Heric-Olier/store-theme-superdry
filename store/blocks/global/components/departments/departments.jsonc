  "flex-layout.col#categoryresults--content": {
    "title": "category results content",
    "children": [
      "flex-layout.row#banner__images--container--desktop",
      "flex-layout.row#departmentbreadcrumb",
      "flex-layout.row#galleryswitcher--desktop",
      "search-content#resultscontent--products"
    ],
    "props": {
      "blockClass": "search__content--container",
      "width": "84%",
      "preserveLayoutOnMobile": true,
      "horizontalAlign": "between"
    }
  },
  "flex-layout.col#categoryresults--content--mobile": {
    "title": "category results content mobile",
    "children": [
      "flex-layout.row#departmentbreadcrumb",
      "flex-layout.row#galleryswitcher--mobile",
      "flex-layout.row#banner__images--container--mobile",
      "search-content#resultscontent--products"
    ],
    "props": {
      "blockClass": "search__content--container",
      "width": "100%",
      "preserveLayoutOnMobile": true,
      "horizontalAlign": "between"
    }
  },

  "order-by.v2": {
    "props": {
      "blockClass": "search__order-by--container",
      "width": "50%"
    }
  },
  "search-content#resultscontent--products": {
    "title": "search content",
    "blocks": [
      "gallery#department",
      "not-found"
    ]
  },
  "gallery#department": {
    "title": "gallery",
    "props": {
      "layouts": [
        {
          "name": "desktop-grid",
          "component": "SummaryGrid",
          "itemsPerRow": {
            "desktop": 4,
            "tablet": 2,
            "phone": 2
          }
        },
        {
          "name": "desktop-list",
          "component": "SummaryList",
          "itemsPerRow": 1
        }
      ],
      "SummaryGrid": "product-summary.shelf#search__grid",
      "SummaryList": "product-summary.shelf#search__list"
    }
  },
  "product-summary.shelf#search__grid": {
    "title": "product summary grid",
    "props": {
      "blockClass": "search__grid--product-summary",
      "alignment": "center"
    },
    "children": [
      "product-summary-image",
      "product-summary-name",
      "product-rating-inline",
      "product-summary-space",
      "product-summary-price",
      "add-to-cart-button",
      "modal-trigger#quickview__modal"
    ]
  },
  "product-summary.shelf#search__list": {
    "title": "product summary list",
    "props": {
      "blockClass": "search__list--product-summary"
    },
    "children": [
      "product-summary-image",
      "product-summary-name",
      "product-rating-inline",
      "product-summary-space",
      "product-summary-price",
      "add-to-cart-button",
      "modal-trigger#quickview__modal"
    ]
  },
  "modal-trigger#quickview__modal": {
    "title": "quick view modal trigger",
    "props": {
      "blockClass": "quickview__modal--trigger"
    },
    "children": [
      "image#quickview__modal--button",
      "modal-layout#quickview__modal--layout"
    ]
  },
  "image#quickview__modal--button": {
    "title": "quick view modal button",
    "props": {
      "src": "assets/img/icon-eye.svg",
      "alt": "quick view modal button",
      "width": 22,
      "blockClass": "quickview__modal--button"
    }
  },
  "modal-layout#quickview__modal--layout": {
    "title": "quick view modal layout",
    "props": {
      "blockClass": "quickview__modal--layout",
      "title": "Quick View",
      "showCloseButton": true,
      "width": "70%"
    },
    "children": [
      "flex-layout.row#quickview__modal--row"
      
    ]
  },
  "flex-layout.row#quickview__modal--row": {
    "title": "quick view modal row",
    "children": [
      "flex-layout.col#quickview__modal--image",
      "flex-layout.col#quickview__modal--info"
    ],
    "props": {
      "blockClass": "quickview__modal--row",
      "paddingTop": 6,
      "paddingBottom": 6
      
    }
  },
  "flex-layout.col#quickview__modal--image": {
    "title": "quick view modal image",
    "children": [
      "product-images"
    ],
    "props": {
      "blockClass": "quickview__modal--image",
      "paddingLeft": 6,
      "paddingRight": 6
    }
  },
  "product-images": {
    "title": "quick view modal product images",
    "props": {
      "blockClass": "quickview__modal--image"
    }
  },
  "flex-layout.col#quickview__modal--info": {
    "title": "quick view modal info",
    "children": [
      "close-button",
      "vtex.store-components:product-name",
      "vtex.store-components:product-brand",
      "product-identifier.product",
      "product-description",
      "product-price",
      "product-separator",
      "product-rating-inline",
      "share",
      "product-separator",
      "product-quantity",
      "add-to-cart-button"
    ],
    "props": {
      "blockClass": "quickview__modal--info",
      "paddingLeft": 6,
      "paddingRight": 6
    }
  },
  "vtex.store-components:product-name": {
    "title": "quick view modal product name",
    "props": {
      "tag": "h3",
      "blockClass": "quickview__modal--product-name"
    }
  },

  "flex-layout.row#departmentbreadcrumb": {
    "title": "department breadcrumb row",
    "children": [
      "flex-layout.col#departmentbreadcrumb"
    ]
  },
  "flex-layout.col#departmentbreadcrumb": {
    "title": "department breadcrumb col",
    "children": [
      "breadcrumb.search"
    ]
  },
  "breadcrumb.search": {
    "props": {
      "blockClass": "breadcrumb__search"
    }
  },
  "flex-layout.row#galleryswitcher--desktop": {
    "title": "gallery switcher row",
    "children": [
      "flex-layout.col#gallery-layout-switcher#desktop",
      "flex-layout.col#order-by.v2"
    ],
    "props": {
      "blockClass": "gallery-switcher__container",
      "width": "100%",
      "preventHorizontalStretch": true
    }
  },
  "flex-layout.row#galleryswitcher--mobile": {
    "title": "gallery switcher row",
    "children": [
      "filter-navigator.v3#mobile",
      "flex-layout.col#container--gallery&order"
    ],
    "props": {
      "blockClass": "gallery-switcher__container--mobile",
      "width": "100%",
      "preventHorizontalStretch": true,
      "sticky": true
    }
  },
  "filter-navigator.v3#mobile": {
    "title": "filter navigator mobile",
    "blocks": [
      "sidebar-close-button"
    ],
    "props": {
      "blockClass": "filter-navigator__mobile"
    }
  },
  "flex-layout.col#container--gallery&order": {
    "title": "gallery switcher row",
    "children": [
      "flex-layout.col#gallery-layout-switcher#desktop",
      "flex-layout.col#order-by.v2"
    ],
    "props": {
      "blockClass": "gallery-switcher__order--container",
      "width": "100%",
      "preventHorizontalStretch": true
    }
  },

  "flex-layout.col#gallery-layout-switcher#desktop": {
    "title": "gallery switcher desktop",
    "children": [
      "gallery-layout-switcher#desktop"
    ],
    "props": {
      "blockClass": "gallery-switcher__desktop"
    }
  },
  "flex-layout.col#order-by.v2": {
    "title": "order by v2",
    "children": [
      "order-by.v2"
    ],
    "props": {
      "blockClass": "order-by__v2",
      "width": "100%"
    }
  },
  "gallery-layout-switcher#desktop": {
    "children": [
      "gallery-layout-option#grid",
      "gallery-layout-option#list"
    ],
    "props": {
      "blockClass": "gallery-switcher__desktop"
    }
  },
  "gallery-layout-option#grid": {
    "props": {
      "name": "desktop-grid"
    },
    "children": [
      "image#icon-grid"
    ]
  },
  "gallery-layout-option#list": {
    "props": {
      "name": "desktop-list"
    },
    "children": [
      "image#icon-list"
    ]
  },
  "image#icon-grid": {
    "props": {
      "src": "assets/img/icon-grid.svg",
      "alt": "icon grid",
      "width": 16,
      "blockClass": "icon-grid"
    }
  },
  "image#icon-list": {
    "props": {
      "src": "assets/img/icon-list.svg",
      "alt": "icon list",
      "width": 16,
      "blockClass": "icon-list"
    }
  }
}
