{
  "store.search#department": {
    "props": {
      "context": {
        "skusFilter": "FIRST_AVAILABLE",
        "simulationBehavior": "default"
      }
    },
    "blocks": [
      "search-result-layout#department"
    ]
  },
  "search-result-layout#department": {
    "title": "search result layout",
    "blocks": [
      "search-result-layout.desktop#department",
      "search-result-layout.mobile#department",
      "search-not-found-layout"
    ]
  },
  "search-result-layout.desktop#department": {
    "title": "search result layout desktop",
    "children": [
      "flex-layout.row#categoryresults--desktop"
    ],
    "props": {
      "blockClass": "search__layout--desktop--container",
      "width": "100%",
      "fullWidth": true,
      "preventRouteChange": true
    }
  },
  "search-result-layout.mobile#department": {
    "title": "search result layout mobile",
    "children": [
      "flex-layout.row#categoryresults--mobile"
    ],
    "props": {
      "blockClass": "search__layout--mobile--container",
      "width": "100%",
      "fullWidth": true,
      "preventRouteChange": true
    }
  },
  "flex-layout.row#categoryresults--mobile": {
    "title": "category results",
    "children": [
      "responsive-layout.mobile#categoryresults--content"
    ],
    "props": {
      "blockClass": "search__layout--container",
      "width": "100%",
      "fullWidth": true,
      "preventRouteChange": true,
      "preserveLayoutOnMobile": true
    }
  },
  "flex-layout.row#categoryresults--desktop": {
    "title": "category results",
    "children": [
      "responsive-layout.desktop#categoryresults--filter",
      "responsive-layout.desktop#categoryresults--content"
    ],
    "props": {
      "blockClass": "search__layout--container",
      "width": "100%",
      "fullWidth": true,
      "preventRouteChange": true,
      "preserveLayoutOnMobile": true
    }
  },
  "responsive-layout.desktop#categoryresults--filter": {
    "title": "category results filter",
    "children": [
      "flex-layout.col#categoryresults--filter"
    ],
    "props": {
      "blockClass": "search__filter--container",
      "width": "16%",
      "horizontalAlign": "between"
    }
  },
  "responsive-layout.desktop#categoryresults--content": {
    "title": "category results content",
    "children": [
      "flex-layout.col#categoryresults--content"
    ],
    "props": {
      "blockClass": "search__content--container",
      "width": "84%",
      "horizontalAlign": "between"
    }
  },
  "responsive-layout.mobile#categoryresults--filter": {
    "title": "category results filter",
    "children": [
      "flex-layout.row#galleryswitcher--mobile"
    ],
    "props": {
      "blockClass": "search__filter--container--mobile",
      "width": "100%",
      "horizontalAlign": "between"
    }
  },
  "responsive-layout.mobile#categoryresults--content": {
    "title": "category results content",
    "children": [
      "flex-layout.col#categoryresults--content--mobile"
    ],
    "props": {
      "blockClass": "search__content--container--mobile",
      "width": "100%",
      "horizontalAlign": "between"
    }
  },
  "flex-layout.col#categoryresults--filter": {
    "title": "category results filter",
    "children": [
      "filter-navigator.v3"
    ],
    "props": {
      "blockClass": "search__filter--container",
      "width": "16%",
      "horizontalAlign": "between"
    }
  },

  "filter-navigator.v3": {
    "blocks": [
      "sidebar-close-button"
    ],
    "props": {
      "navigationTypeOnMobile": "collapsible",
      "truncateFilters": true
    }
  },

  "flex-layout.row#banner__images--container--desktop": {
